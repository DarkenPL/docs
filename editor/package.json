{
  "name": "editor",
  "version": "0.0.3",
  "description": "The Paperize serverless web application",
  "main": "lib/main.js",
  "scripts": {
    "clean": "rm -rf build",
    "start": "concurrently \"npm run webpack-watch\" \"npm run server\"",
    "server": "http-server build",
    "webpack-watch": "webpack --watch",
    "dev-cypress": "NODE_ENV=test concurrently --kill-others \"npm start\" \"cypress open\" ",
    "dev-mocha": "NODE_ENV=test mocha-webpack --webpack-config webpack.test.config.js test/test.coffee --watch",
    "test-unit": "NODE_ENV=test mocha-webpack --webpack-config webpack.test.config.js test/test.coffee",
    "build-test-integration": "npm run clean && NODE_ENV=test webpack",
    "test-integration": "npm run build-test-integration && concurrently --kill-others \"npm run server\" \"cypress run\"",
    "test": "npm run test-unit && npm run test-integration",
    "create-video": "npm run build-test-integration && concurrently --kill-others \"npm run server\" \"cypress run --config videoRecording=true\"",
    "copy-video": "cp cypress/videos/* build/releases/10-14-17.mp4",
    "finalize-prod": "coffee finalize.coffee",
    "build-prod": "npm run clean && NODE_ENV=production webpack -p",
    "deploy-prod": "coffee deploy.coffee",
    "bump-prod": "npm run build-prod && npm run deploy-prod",
    "release": "npm run finalize-prod && npm run create-video && npm run build-prod && npm run copy-video && npm run deploy-prod"
  },
  "author": "Loren Norman <loren@oilandrope.com>",
  "license": "ISC",
  "devDependencies": {
    "ajv": "^5.2.3",
    "aws-sdk": "^2.88.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "coffee-loader": "^0.8.0",
    "coffeescript": "^2.0.0",
    "concurrently": "^3.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "core-util-is": "^1.0.2",
    "css-loader": "^0.26.2",
    "cypress": "^1.1.4",
    "express": "^4.14.1",
    "file-api": "^0.10.4",
    "http-server": "^0.10.0",
    "leveldown": "^2.0.0",
    "mime-types": "^2.1.14",
    "mocha": "^3.5.3",
    "mocha-webpack": "^1.0.1",
    "pouchdb-adapter-memory": "^6.3.4",
    "pug": "^2.0.0-beta11",
    "recursive-readdir": "^2.2.1",
    "semver": "^5.3.0",
    "sinon": "^4.0.0",
    "sinon-chai": "^2.14.0",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "util-deprecate": "^1.0.2",
    "vue-template-compiler": "^2.5.5",
    "webpack": "^2.7.0",
    "webpack-dev-server": "^2.5.1"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "bluebird": "^3.5.0",
    "embed-js": "^4.2.1",
    "event-emitter": "^0.3.4",
    "jspdf": "^1.3.5",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.4",
    "md5": "^2.2.1",
    "pouchdb-adapter-http": "^6.3.4",
    "pouchdb-adapter-idb": "^6.3.4",
    "pouchdb-adapter-websql": "^6.3.4",
    "pouchdb-core": "^6.3.4",
    "pouchdb-find": "^6.3.4",
    "pouchdb-mapreduce": "^6.3.4",
    "pouchdb-replication": "^6.3.4",
    "uuid": "^3.1.0",
    "vue": "^2.5.5",
    "vue-async-computed": "^3.3.1",
    "vue-js-modal": "^1.2.11",
    "vue-loader": "^11.1.3",
    "vue-router": "^2.3.0",
    "vue-simple-spinner": "^1.2.7",
    "vuedraggable": "^2.15.0",
    "vuex": "^2.3.1"
  }
}
