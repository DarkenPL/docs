{
  "name": "editor",
  "version": "0.0.1",
  "description": "The web frontend game and template editor",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run start-webpack\" \"npm run start-server\"",
    "start-webpack": "webpack --watch",
    "start-server": "http-server build",
    "cypress-record": "cypress run; cp cypress/videos/*.mp4 build/releases/8-4-17.mp4",
    "test": "NODE_ENV=test npm start",
    "ci": "NODE_ENV=test concurrently --kill-others \"npm run start-server\" \"cypress run\" ",
    "clean": "rm -rf build",
    "build": "npm run clean && npm run build-app && npm run build-video",
    "build-app": "webpack",
    "build-video": "concurrently --kill-others \"npm run start-server\" \"npm run cypress-record\" ",
    "deploy": "coffee deploy.coffee",
    "release": "NODE_ENV=production npm run build -- -p && npm run deploy",
    "produce-video": "npm run ci",
    "copy-video": ""
  },
  "author": "Loren Norman <loren@oilandrope.com>",
  "license": "ISC",
  "devDependencies": {
    "ajv": "^5.2.2",
    "aws-sdk": "^2.88.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.24.1",
    "coffee-script": "^1.12.7",
    "concurrently": "^3.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "core-util-is": "^1.0.2",
    "css-loader": "^0.26.2",
    "cypress": "^0.10.1",
    "express": "^4.14.1",
    "http-server": "^0.10.0",
    "mime-types": "^2.1.14",
    "pug": "^2.0.0-beta11",
    "recursive-readdir": "^2.2.1",
    "semver": "^5.3.0",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "util-deprecate": "^1.0.2",
    "vue-template-compiler": "^2.4.1",
    "webpack": "^2.7.0",
    "webpack-dev-server": "^2.5.1"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "bluebird": "^3.5.0",
    "embed-js": "^4.2.1",
    "event-emitter": "^0.3.4",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.4",
    "uuid": "^3.1.0",
    "vue": "^2.4.1",
    "vue-loader": "^11.1.3",
    "vue-router": "^2.3.0",
    "vuex": "^2.3.1"
  }
}
